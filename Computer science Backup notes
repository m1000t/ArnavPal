// let caps= prompt("first sentence");
// let Smalls= prompt("Second sentence");
// console.log(caps,Smalls)

//task 1 ends 


//task 2 starts 

let sen = prompt("First sentence ") //5
let vowel = 0 //2
let space = 0
let cons = 0 //3 
let punc = 0 
// 0123456789

for(let i = 0; i < sen.length; i++){
if (sen.at(i) == "a" || sen.at(i) == "e" || sen.at(i) == "i" || sen.at(i) == "o" || sen.at(i) == "u" ){
  vowel += 1  
  }
  else if(sen.at(i) == " "){
    space += 1 
    
  }
  else if(sen.at(i) == "." || sen.at(i) == ";" || sen.at(i) == ":"){
    punc +=1
  }

  else{
    cons += 1
  }
}

console.log("Vowels " + vowel)
console.log("Space  " + space)
console.log("Punctuation  " + punc)
console.log("Consoants  " + cons)

