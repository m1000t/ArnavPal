/*
  Name:        Arnav Pal 
  Purpose:     Generates a new file for use in the ICS3U course

  Author:      
  Created:     28-Sep-2022
  Updated:     10/13/2022
*/

require('./index.js');

let smallest, largest, roundedaverage;
let numOfLoops = parseInt(prompt("How many numbers are u entering"))
let counter = 0 
let a,b

do{
  if(counter< 2){
    a= parseInt(prompt(""))
    b= parseInt(prompt(""))
    smallest = Math.min(a,b)//(1,3) ==1
    largest= Math.max(a,b)//(1,3) == 3
    roundedaverage = (a+b)
    counter = 2
  }
  else{
    b= parseInt(prompt(""))
    console.log(b)
    smallest = Math.min(smallest,b)
    largest = Math.max(largest,b)
    roundedaverage += b
    counter +=1
  }
} while (counter< numOfLoops);

console.log("Smallest" +smallest)
console.log("Largest"+largest)
console.log("roundedaverage"+ roundedaverage)




